{
  "report_title": "Health Check Report",
  "sections": [
    {
      "title": "Overall Results",
      "fields": [
        { "label": "Accuracy", "path": "$.accuracy", "default": "N/A" }
      ]
    },
    {
      "title": "Vitals",
      "fields": [
        {
          "label": "Heart Rate",
          "path": "$.vitalsMap.vitals.heart_rate",
          "default": "N/A",
          "classifications": [
            { "label": "Low", "min": 0, "max": 59 },
            { "label": "Normal", "min": 60, "max": 100 },
            { "label": "High", "min": 101, "max": 200 }
          ]
        },
        {
          "label": "Blood Pressure",
          "path": "$.vitalsMap.vitals.blood_pressure",
          "default": "N/A"
        }
      ]
    },
    {
      "title": "Body Composition",
      "fields": [
        {
          "label": "BMI",
          "path": "$.bodyCompositionData.BMI",
          "default": "N/A",
          "classifications": [
            { "label": "Underweight", "min": 0, "max": 18.4 },
            { "label": "Normal", "min": 18.5, "max": 24.9 },
            { "label": "Overweight", "min": 25, "max": 29.9 },
            { "label": "Obese", "min": 30, "max": 100 }
          ]
        }
      ]
    }
  ]
}
